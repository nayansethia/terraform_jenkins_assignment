pipeline {
    agent any
    stages
    {
        stage('checkout from Git')
    {
        steps{
            echo "Checking out from git"
        
        }
    }
        
    stage('Building')
    {
        steps{
        echo 'This is the build stage'
        }
        
    }
    stage('At Jenkins')
    {
        steps{
            echo "At jenkins now"
        }
    }
    stage('AT s3')
    {
    steps{
   s3CopyArtifact(buildSelector: lastSuccessful(), excludeFilter: '', filter: 'git_source_file.py', flatten: false, optional: false, projectName: 'ns_pipeline', target: 'ns-jenkins-bucket')
     }
    }
}
}
